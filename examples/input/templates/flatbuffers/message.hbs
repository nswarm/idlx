{{> message_type}} {{name}} {
{{#each fields}}
{{#if is_map}}
    {{error "field {}: maps are unsupported in fbs." field_name}}
{{else}}
    {{#if is_array}}
    {{field_name}}: [{{relative_type}}]
    {{else}}
    {{field_name}}: {{relative_type}}
    {{~/if~}}
    {{> field_separator}}
{{/if}}
{{/each}}
}
