{{> message_type}} {{name}} {
{{#each fields}}
{{#if is_map}}
    *** ERROR: {{field_name}} - maps are unsupported in fbs. ***
{{else}}
    {{#if is_array}}
    {{field_name}}: [{{relative_type}}]
    {{else}}
    {{field_name}}: {{relative_type}}
    {{~/if~}}
    {{> field_separator}}
{{/if}}
{{/each}}
}
